# Production Environment Configuration
NODE_ENV=production
IS_ELECTRON=true
VITE_APP_NAME="Live Simulation Config Explorer"
VITE_APP_VERSION="0.1.0"
VITE_API_BASE_URL=""
VITE_ELECTRON_DEV=false
VITE_HOT_RELOAD=false
VITE_DEBUG=false
VITE_LOG_LEVEL="error"

# Production-specific settings
VITE_DEV_TOOLS=false
VITE_SOURCEMAPS=false
VITE_DEV_SERVER_HOST=""
VITE_DEV_SERVER_PORT=""

# Simulation configuration defaults
VITE_DEFAULT_SIMULATION_CONFIG="config.yaml"
VITE_SIMULATION_DATA_PATH="./simulations"
VITE_MAX_SIMULATION_STEPS=50000

# Performance settings
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_PROFILING_ENABLED=false

# Feature flags
VITE_ENABLE_ADVANCED_ANALYTICS=true
VITE_ENABLE_REAL_TIME_COLLABORATION=false
VITE_ENABLE_CLOUD_BACKUP=false